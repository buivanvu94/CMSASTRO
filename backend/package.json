{
  "name": "cms-backend",
  "version": "1.0.0",
  "description": "CMS System Backend API",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "db:sync": "node force-sync-db.js",
    "db:reset": "node reset-database.js",
    "db:indexes": "node src/migrations/add-indexes.js",
    "db:dedup-indexes": "node src/migrations/deduplicate-indexes.js",
    "db:split-product-categories": "node src/migrations/split-product-categories.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": ["cms", "api", "express", "sequelize"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.2",
    "mysql2": "^3.6.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.16"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "fast-check": "^3.15.0",
    "@types/jest": "^29.5.11",
    "cross-env": "^7.0.3"
  }
}
